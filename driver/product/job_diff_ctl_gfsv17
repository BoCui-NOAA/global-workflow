filelist="
000001"

for station in $filelist; do

  file=bufr.${station}.$PDY$cyc

  $debufr $COM/gfs.$PDY/$cyc/products/atmos/bufr/$file
  mv $file.debufr.out out_mod_$file

  $debufr $COMctl/gfs.$PDY/bufr.t${cyc}z/$file              
  mv $file.debufr.out out_ctl_$file

  diff out_mod_$file out_ctl_$file

  echo $file
  diff $COM/gfs.$PDY/$cyc/products/atmos/bufr/$file $COMctl/gfs.$PDY/bufr.t${cyc}z/$file                  

# ls $COM/gfs.$PDY/$cyc/products/atmos/bufr/$file
# ls  $COMctl/gfs.$PDY/bufr.t${cyc}z/$file
  echo

done

